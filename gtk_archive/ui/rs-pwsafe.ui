              <object class="GtkLabel" id="description">
                <property name="label">Description text here</property>
                <property name="lines">2</property>
                <property name="natural-wrap-mode">word</property>
                <property name="wrap">True</property>
              </object>
            </child>
          </object>
        </child>
        <child>
